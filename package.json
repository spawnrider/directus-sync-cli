{
  "name": "directus-sync-cli",
  "version": "0.0.5",
  "description": "Directus cli to sync envs",
  "author": "@spawnrider,@Aqwarium",
  "bin": {
    "directus-sync-cli": "./bin/run"
  },
  "homepage": "https://github.com/ArthurSamy/directus-sync-cli",
  "license": "MIT",
  "main": "dist/index.js",
  "repository": "spawnrider/directus-sync-cli",
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "dependencies": {
    "@oclif/core": "^1",
    "@oclif/plugin-help": "^5",
    "@oclif/plugin-plugins": "^2.0.1",
    "axios": "^0.27.2",
    "chalk": "^4.1.2",
    "cli-table": "^0.3.11",
    "form-data": "^4.0.0",
    "inquirer": "^8.2.4",
    "listr": "^0.14.3"
  },
  "devDependencies": {
    "@types/inquirer": "^8.2.1",
    "@oclif/test": "^2",
    "@types/chai": "^4",
    "@types/cli-table": "^0.3.0",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.9.4",
    "chai": "^4",
    "eslint": "^7.32.0",
    "eslint-config-oclif": "^4",
    "eslint-config-oclif-typescript": "^1.0.2",
    "globby": "^11",
    "mocha": "^9",
    "oclif": "^3",
    "shx": "^0.3.3",
    "ts-node": "^10.2.1",
    "tslib": "^2.3.1",
    "typescript": "^4.9.4"
  },
  "oclif": {
    "bin": "directus-sync-cli",
    "dirname": "directus-sync-cli",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help"
    ],
    "topicSeparator": " ",
    "topics": {
      "env": {
        "description": "Manage directus conf"
      },
      "status": {
        "description": "status directus env"
      },
      "sync": {
        "description": "sync directus env"
      }
    }
  },
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint . --ext .ts --config .eslintrc",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "npm lint",
    "prepack": "npm run build && oclif manifest && oclif readme",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "bugs": "https://github.com/spawnrider/directus-sync-cli/issues",
  "keywords": [
    "directus",
    "cli",
    "environment",
    "sync",
    "CMS",
    "snapshot"
  ],
  "types": "dist/index.d.ts"
}
